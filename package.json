{
  "name": "metasalt-backend",
  "version": "1.0.0",
  "description": "This is a Metasalt Serverless backend service",
  "scripts": {
      "start": "node --max-old-space-size=8192 node_modules/serverless/bin/serverless.js offline",
      "deploy:dev": "sls deploy --verbose",
      "deploy:staging": "sls deploy --verbose --stage staging",
      "deploy:prod": "sls deploy --verbose --stage prod"
  },
  "devDependencies": {
      "@types/aws-lambda": "^8.10.92",
      "@types/aws-serverless-express": "^3.3.5",
      "@types/cors": "^2.8.12",
      "@types/node": "^17.0.21",
      "@types/swagger-jsdoc": "^6.0.1",
      "@typescript-eslint/eslint-plugin": "^5.8.1",
      "@typescript-eslint/parser": "^5.8.1",
      "eslint": "^8.5.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.25.3",
      "eslint-plugin-prettier": "^4.0.0",
      "increase-memory-limit": "^1.0.7",
      "prettier": "^2.5.1",
      "serverless": "^3.7.1",
      "serverless-dotenv-plugin": "^3.12.2",
      "serverless-offline": "^8.5.0",
      "serverless-plugin-typescript": "2.1.1",
      "serverless-tscpaths": "^0.0.6",
      "ts-node": "^10.5.0",
      "typescript": "^4.5.5"
  },
  "dependencies": {
      "aws-sdk": "^2.1081.0",
      "aws-serverless-express": "^3.4.0",
      "axios": "^0.26.0",
      "cognito-express": "^3.0.1",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "country-code-lookup": "^0.0.20",
      "express": "^4.17.3",
      "morgan": "^1.10.0",
      "swagger-jsdoc": "^6.1.0",
      "uuidv4": "^6.2.12"
  },
  "prettier": {
      "singleQuote": true,
      "trailingComma": "all",
      "printWidth": 120
  }
}
